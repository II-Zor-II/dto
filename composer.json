{
  "name": "dto/dto",
  "description": "Data Transfer Object: define object structures using the JSON Schema spec to hold structured data",
  "type": "project",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.5.9"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/sqills/PhpSpecCodeCoverageExtension"
    }
  ],
  "require-dev": {
    "phpspec/phpspec": "^3.2",
    "phpunit/phpunit": "^5.7",
    "sqills/phpspec-code-coverage": "^3.0",
    "phpunit/php-code-coverage": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Dto\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": { "DtoTest\\": "tests/" }
  }
}
